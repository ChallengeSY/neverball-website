@import url("../global.css");


#navigation{
	position: absolute;
	top: 0px;
	text-align: center;
	margin: 0 auto;
}



#primary{
	position: absolute;
	width: 100%;
	margin: 0;
	padding: 0;

}

#primary li{
	display: inline;
	padding: 0;
	margin: 0;
	text-align: center;
	font-size:medium;
	font-weight: bolder;
	list-style-type: none;

}

#primary li a, #primary li span{
	display: block;
	width: 130px;
	height: 30px;
	float: left;
	margin: 1px;
	padding-left: 0;
	padding-right: 0;
	padding-bottom: 0;
	background: #1b481b;
	text-decoration: none;
	border: 1px solid #303030;
}

#primary li a{
	padding-top: 10px;
	color: #ffffff;
}

#primary li span{
	padding-top: 20px;
	color: #ffaf00;
	background-image: url("../../images/neverstyle/widget.png")repeat-x top;
}

#primary li a:hover{
	padding-top: 20px;
}

#primary li span:before{
	display:block;
	height: 18px;
	margin-top: -8px;
	content: url(../../images/neverstyle/widget.png);
}


body{
	background: #FF4949 url("../../images/neverstyle/background-ie.png") repeat-x;
	color: #FFF;
	margin: 0 auto;
}
#banner
{
	width: 326px;
	height: 52px;
	margin-top: 50px;
	margin-bottom: 20px;
	background: url("../../images/neverstyle/heading.png") no-repeat;
	margin-left: auto;
	margin-right: auto;
}

#contents
{
	background: #802525 url("../../images/neverstyle/content-back.png") repeat-x;
	padding: 32px;
	border: 1px solid #303030;
}


#footer
{

	margin-top: 40px;
	position: absolute;
/*	left:50%;*/
/*	bottom: 0;*/
/*	width:700px;*/
	height: 50px;
/*	margin-left:-350px;*/
	line-height: 50px;
	width: 100%;
	background: #802525;
	text-align: center;
	border: 1px solid #303030;
}

/*********************/
#contents a{
	background-color: inherit;
	color: #F55;
}

#contents a:hover{
	background-color: inherit;
	color: #FFAF00;
}

#contents p,pre{
	margin-left: 20px;
}

#contents h2{
	margin-top: 32px;
	color: #ffaf00;
	background-color: inherit;
	font-size:large;
	font-weight: bolder;
}

#contents h3{
	margin-top: 2px;
	/*margin-left: 10px;*/
}


#contents ul
{
	list-style-type: none;
	padding: 0;
	margin: 0;
}

#contents li
{
	margin-left: 2em;
	margin-top: 0.5em;
	padding-left: 18px;
	background: url(../../images/neverstyle/ball_small.png) no-repeat center left;
}

dt
{
	color: #ffaf00;
	background-color: inherit;
	font-weight: bolder;
}

#contents dd ul li
{
	background-image: none;
}

q
{
	font-style: italic;
}

code,samp
{
	font-size: large;
}

.style-img-link
{
	margin-bottom: 32px;
}

.dl-img-align
{
	vertical-align: middle;
	padding-left: 0.5em;
	padding-right: 0.5em;
}

.mime-type
{
	float: right;
	padding-right: 8em;
}

.logo
{
	float: left;
}

.none
{
	display: none;
}

.thumbimgs a img {margin: 0 8px 16px;}
.thumbimgs a {border: 2px solid #F55;}
.thumbimgs a:hover {border: 2px solid #FFAF00}
.style-img-link {text-align: center;}

